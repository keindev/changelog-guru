{
  "name": "changelog-guru",
  "version": "4.0.8",
  "description": "Git changelog generator",
  "license": "MIT",
  "homepage": "https://github.com/keindev/changelog-guru#readme",
  "author": {
    "name": "Daniil Ryazanov",
    "email": "kein@tagproject.ru"
  },
  "keywords": [
    "automated",
    "changelog",
    "cli",
    "generator",
    "github"
  ],
  "bugs": "https://github.com/keindev/changelog-guru/issues",
  "types": "./lib/index.d.ts",
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./lib/utils/*": "./lib/utils/*.js",
    "./lib/core/*": "./lib/core/*.js"
  },
  "bin": {
    "changelog": "bin/changelog.js",
    "changelog-guru": "bin/changelog.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keindev/changelog-guru.git"
  },
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:ts": "rimraf lib && tsc --extendedDiagnostics",
    "generate": "npm-run-all generate:*",
    "generate:changelog": "node --loader ts-node/esm src/cli/bin.ts generate --bump",
    "generate:ghinfo": "ghinfo generate -d media -t utils",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "rimraf coverage && eslint src --ext .ts",
    "lint:spell": "cspell -c .vscode/cspell.json --no-summary \"**/*.{js,ts,tsx,md,yml,json}\"",
    "prepare": "npm-run-all prepare:*",
    "prepare:config": "ts-package-shared-config",
    "prepare:docs": "docs-shared-config",
    "prepare:husky": "husky install",
    "prepare:vscode": "vscode-shared-config",
    "release": "npm-run-all prepare lint test build generate",
    "test": "npm-run-all test:*",
    "test:jest": "node --experimental-vm-modules node_modules/.bin/jest"
  },
  "dependencies": {
    "cosmiconfig": "^8.0.0",
    "deepmerge": "^4.2.2",
    "dotenv": "^16.0.3",
    "findup-sync": "^5.0.0",
    "gh-gql": "^4.0.1",
    "package-json-helper": "^4.0.6",
    "string-lookup-manager": "^3.0.8",
    "tasktree-cli": "^6.0.5",
    "universal-user-agent": "^7.0.1",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@tagproject/docs-shared-config": "^1.1.2",
    "@tagproject/ts-package-shared-config": "^10.0.2",
    "@tagproject/vscode-shared-config": "^2.0.5",
    "@types/findup-sync": "^4.0.2",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/semver": "^7.3.13",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "cspell": "^6.14.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "ghinfo": "^3.0.8",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}

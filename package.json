{
  "name": "changelog-guru",
  "version": "2.0.5",
  "description": "Git changelog generator",
  "homepage": "https://github.com/keindev/changelog-guru#readme",
  "license": "MIT",
  "author": {
    "name": "Daniil Ryazanov",
    "email": "kein@tagproject.ru"
  },
  "keywords": [
    "automated",
    "changelog",
    "generator",
    "cli",
    "github"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "changelog-guru": "bin/changelog",
    "changelog": "bin/changelog"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keindev/changelog-guru.git"
  },
  "bugs": {
    "url": "https://github.com/keindev/changelog-guru/issues"
  },
  "scripts": {
    "prepare:husky": "husky install",
    "prepare:clean": "rimraf lib coverage",
    "prepare": "npm-run-all prepare:*",
    "check:lint": "eslint \"src/**\" --ext .ts",
    "check:spell": "cspell -c .vscode/cspell.json --no-summary \"README.md\" \"src/**/*.ts\"",
    "check": "npm-run-all check:*",
    "test": "jest",
    "generate:docs": "rimraf docs/api && typedoc",
    "generate:changelog": "ts-node-script src/cli/bin.ts generate --bump",
    "generate:ghinfo": "ghinfo generate -d media -t utils",
    "generate": "npm-run-all generate:changelog generate:docs generate:ghinfo",
    "build": "tsc",
    "release": "npm-run-all prepare check test build generate"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "cosmiconfig": "^7.0.0",
    "deepmerge": "^4.2.2",
    "dotenv": "^8.2.0",
    "findup-sync": "^4.0.0",
    "gh-gql": "^1.4.2",
    "semver": "^7.3.5",
    "string-lookup-manager": "^1.1.1",
    "tasktree-cli": "^4.0.1",
    "universal-user-agent": "^6.0.0",
    "write-pkg": "^4.0.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/faker": "^5.5.0",
    "@types/findup-sync": "^2.0.2",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/semver": "^7.3.4",
    "@types/yargs": "^16.0.1",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "cspell": "^5.3.9",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-promise": "^4.3.1",
    "faker": "^5.5.2",
    "ghinfo": "^1.0.6",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "type-fest": "^1.0.1",
    "typedoc": "^0.20.34",
    "typedoc-plugin-markdown": "^3.6.0",
    "typescript": "4.2.3"
  }
}

{
    "name": "changelog-guru",
    "version": "1.0.0",
    "description": "Git changelog generator",
    "homepage": "https://github.com/keindev/changelog-guru#readme",
    "license": "MIT",
    "author": {
        "name": "Daniil Ryazanov",
        "email": "opensource@tagproject.ru"
    },
    "keywords": [
        "changelog",
        "generator",
        "cli",
        "github",
        "gitlab"
    ],
    "main": "lib/changelog.js",
    "unpkg": "lib/changelog.js",
    "module": "lib/changelog.js",
    "types": "lib/changelog.d.ts",
    "bin": {
        "changelog-guru": "bin/changelog",
        "changelog": "bin/changelog"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/keindev/changelog-guru.git"
    },
    "bugs": {
        "url": "https://github.com/keindev/changelog-guru/issues"
    },
    "scripts": {
        "clean": "rm -rf lib coverage",
        "lint": "eslint src/**/*.ts",
        "test": "jest",
        "debug": "node --nolazy --inspect-brk=9229 bin/changelog",
        "precommit": "npm-run-all clean test",
        "build:ts": "tsc",
        "build": "npm-run-all clean test build:*"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "dependencies": {
        "@octokit/rest": "^16.28.4",
        "commander": "^2.20.0",
        "cosmiconfig": "^5.2.1",
        "dotenv": "^8.0.0",
        "findup-sync": "^4.0.0",
        "read-pkg": "^5.2.0",
        "semver": "^6.2.0",
        "tasktree-cli": "^1.2.1",
        "write-pkg": "^4.0.0"
    },
    "devDependencies": {
        "@types/cosmiconfig": "^5.0.3",
        "@types/dotenv": "^6.1.1",
        "@types/findup-sync": "^2.0.2",
        "@types/jest": "^24.0.15",
        "@types/node": "^12.6.2",
        "@types/read-pkg": "^4.0.0",
        "@types/semver": "^6.0.1",
        "@types/write-pkg": "^3.1.0",
        "@typescript-eslint/eslint-plugin": "^1.12.0",
        "@typescript-eslint/parser": "^1.12.0",
        "eslint": "6.0.1",
        "eslint-config-airbnb-base": "13.2.0",
        "eslint-config-prettier": "^6.0.0",
        "eslint-plugin-import": "^2.18.0",
        "eslint-plugin-jest": "^22.7.2",
        "husky": "^3.0.0",
        "jest": "^24.8.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.18.2",
        "ts-jest": "^24.0.2",
        "typescript": "^3.5.3"
    }
}

{
    "name": "changelog-guru",
    "version": "1.0.0",
    "description": "Git changelog generator",
    "homepage": "https://github.com/keindev/changelog-guru#readme",
    "license": "MIT",
    "author": {
        "name": "Daniil Ryazanov",
        "email": "kein@tagproject.ru"
    },
    "keywords": [
        "automated",
        "changelog",
        "generator",
        "cli",
        "github",
        "gitlab"
    ],
    "main": "lib/changelog.js",
    "module": "lib/changelog.js",
    "types": "lib/changelog.d.ts",
    "bin": {
        "changelog-guru": "bin/changelog",
        "changelog": "bin/changelog"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/keindev/changelog-guru.git"
    },
    "bugs": {
        "url": "https://github.com/keindev/changelog-guru/issues"
    },
    "scripts": {
        "clean": "rimraf lib coverage",
        "lint": "eslint src/**/*.ts --fix",
        "spellcheck": "cspell -c .vscode/cspell.json --no-summary \"*.*\" \"docs/**/*\" \"**/*.ts\" \"**/*.js\"",
        "test": "jest",
        "build:ts": "tsc",
        "build": "npm-run-all clean spellcheck lint test build:*"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "dependencies": {
        "chalk": "^2.4.2",
        "cosmiconfig": "^5.2.1",
        "deepmerge": "^4.2.2",
        "dotenv": "^8.1.0",
        "findup-sync": "^4.0.0",
        "gh-gql": "^0.3.1",
        "read-pkg": "^5.2.0",
        "rimraf": "^3.0.0",
        "semver": "^7.1.1",
        "string-lookup-manager": "^1.1.0",
        "tasktree-cli": "^2.0.2",
        "universal-user-agent": "^4.0.0",
        "write-pkg": "^4.0.0",
        "yargs": "^15.1.0"
    },
    "devDependencies": {
        "@types/cosmiconfig": "^5.0.3",
        "@types/dotenv": "^6.1.1",
        "@types/faker": "^4.1.9",
        "@types/findup-sync": "^2.0.2",
        "@types/jest": "^24.0.18",
        "@types/node": "^12.7.8",
        "@types/semver": "^6.0.2",
        "@types/yargs": "^13.0.3",
        "@typescript-eslint/eslint-plugin": "^2.3.1",
        "@typescript-eslint/parser": "^2.3.1",
        "cspell": "^4.0.30",
        "eslint": "^6.5.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jest": "^22.17.0",
        "faker": "^4.1.0",
        "husky": "^4.2.1",
        "jest": "^24.9.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.19.1",
        "ts-jest": "^24.1.0",
        "typescript": "3.6.3"
    }
}
